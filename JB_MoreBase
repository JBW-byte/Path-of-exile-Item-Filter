	BaseType == "Focused Amulet" "Composite Ring" "Nameless Ring" "Manifold Ring" "Ratcheting Ring" "Helical Ring" "Coral Ring" "Sapphire Ring" "Topaz Ring" "Ruby Ring" "Two-Stone Ring" "Prismatic Ring" "Agate Amulet" "Citrine Amulet" "Turquoise Amulet" "Onyx Amulet" "Marble Amulet" "Blue Pearl Amulet" "Stygian Vise" "Leather Belt" "Lapis Amulet" "Seaglass Amulet" "Ruby Amulet" "Jet Amulet" "Vanguard Belt" "Crystal Belt" "Two-Toned Boots" "Marble Amulet" "Moonstone Ring" "Diamond Ring" "Amber Amulet" "Amethyst Ring" "Jade Amulet" "Rustic Sash" "Unset Ring" "Bone Ring" "Heavy Belt"
	Corrupted True
	Rarity = Rare
	SetBorderColor 255 0 0 225
	SetFontSize 34
